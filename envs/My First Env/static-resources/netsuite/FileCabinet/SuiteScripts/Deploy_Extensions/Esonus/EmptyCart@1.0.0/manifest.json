{
    "name": "EmptyCart",
    "fantasyName": "Custom Empty Cart Extension",
    "vendor": "Esonus",
    "type": "extension",
    "target": "SCA,SCS",
    "target_version": {
        "SCA": ">=19.2.0",
        "SCS": ">=19.2.0"
    },
    "version": "1.0.0",
    "description": "Quickly assist a customer in completely clearing their cart",
    "assets": {
        "img": {
            "files": []
        },
        "fonts": {
            "files": []
        },
        "services": {
            "files": [
                "services/EmptyCart.Service.ss"
            ]
        }
    },
    "configuration": {
        "files": [
            "Modules/EmptyCart/Configuration/EmptyCart.json"
        ]
    },
    "templates": {
        "application": {
            "shopping": {
                "files": [
                    "Modules/EmptyCart/Templates/empty_cart.tpl"
                ]
            }
        }
    },
    "sass": {
        "entry_points": {
            "shopping": "Modules/EmptyCart/Sass/_empty-cart.scss"
        },
        "files": [
            "Modules/EmptyCart/Sass/_empty-cart.scss"
        ]
    },
    "javascript": {
        "entry_points": {
            "shopping": "Modules/EmptyCart/JavaScript/EmptyCart.js"
        },
        "application": {
            "shopping": {
                "files": [
                    "Modules/EmptyCart/JavaScript/EmptyCart.Helper.js",
                    "Modules/EmptyCart/JavaScript/EmptyCart.js",
                    "Modules/EmptyCart/JavaScript/EmptyCart.View.js"
                ]
            }
        }
    },
    "ssp-libraries": {
        "entry_point": "Modules/EmptyCart/SuiteScript/EmptyCart.ServiceController.js",
        "files": [
            "Modules/EmptyCart/SuiteScript/EmptyCart.ServiceController.js"
        ]
    }
}