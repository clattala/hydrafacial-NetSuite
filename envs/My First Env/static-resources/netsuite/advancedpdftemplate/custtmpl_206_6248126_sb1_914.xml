<?xml version="1.0"?><!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
   <head>
      <link name="NotoSans" type="font" subtype="truetype" src="${nsfont.NotoSans_Regular}" src-bold="${nsfont.NotoSans_Bold}" src-italic="${nsfont.NotoSans_Italic}" src-bolditalic="${nsfont.NotoSans_BoldItalic}" bytes="2" />
      <#if .locale == "zh_CN">
      <link name="NotoSansCJKsc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKsc_Regular}" src-bold="${nsfont.NotoSansCJKsc_Bold}" bytes="2" />
      <#elseif .locale == "zh_TW">
      <link name="NotoSansCJKtc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKtc_Regular}" src-bold="${nsfont.NotoSansCJKtc_Bold}" bytes="2" />
      <#elseif .locale == "ja_JP">
      <link name="NotoSansCJKjp" type="font" subtype="opentype" src="${nsfont.NotoSansCJKjp_Regular}" src-bold="${nsfont.NotoSansCJKjp_Bold}" bytes="2" />
      <#elseif .locale == "ko_KR">
      <link name="NotoSansCJKkr" type="font" subtype="opentype" src="${nsfont.NotoSansCJKkr_Regular}" src-bold="${nsfont.NotoSansCJKkr_Bold}" bytes="2" />
      <#elseif .locale == "th_TH">
      <link name="NotoSansThai" type="font" subtype="opentype" src="${nsfont.NotoSansThai_Regular}" src-bold="${nsfont.NotoSansThai_Bold}" bytes="2" />
      </#if>
      <macrolist>
         <macro id="nlheader">
            <table width="100%">
               <tr>
                  <td width="60%"><#if companyInformation.logoUrl?length !=0><img src="${companyInformation.logoUrl}" style="float: left; margin: 7px; width:50%;height:50%" /> </#if></td>
                  
               </tr>
            </table>
            <table width="100%">
               <tr>
                  <td width="60%" style="font-weight:bold;">
                    ${salesorder.billaddress}
                  </td>
                  <td align="right" width="40%" style="font-weight:bold; padding-bottom:10px; margin-bottom:5px;">
                    Hydrafacial Germany GmbH <br />
                    Stichlingstrasse 1 <br />
                    60327 Frankfurt am Main <br />
                    Deutschland <br />
                    E-mail: kundenservice@hydrafacial.com <br />
                    www.hydrafacial.de <br />
                  </td>
               </tr>
            </table>
         </macro> 
		 
         <macro id="nlfooter">
          	<table width="100%">
              <tr>
                 <td width="40%">&nbsp;</td>
                 <td width="30%">&nbsp;</td>
                 <td width="30%" align="right">Seite : <pagenumber/> of <totalpages/></td>
              </tr>
           </table>
            <table style="margin-top:10px;" width="100%">
              <tr>
                  <td width ="30%" style="font-weight:bold"> Hydrafacial Germany GmbH<br/>
                     Registergericht Frankfurt<br/>
                     HRB 127913 </td>
                  <td width = "35%" style="font-weight:bold">Wiesbadener Volksbank <br/>
                     IBAN : DE57 5109 0000 0001 0495 00<br/>
                     BIC : WIBADE5W </td>
                  <td width = "35%" style="font-weight:bold"> Ust-Id.-Nr: DE81454802<br/>
                     WEEE-Reg.-Nr: DE 83534493<br/>
                     Geschaftsfuhrem: Arnold Jonathan,<br/>
                     Eikenberg, Katharina</td>
              </tr>
            </table>
         </macro>
      </macrolist>
      <style type="text/css">
         * {
         font-family: NotoSans, sans-serif;
         }
         table {
         font-size: 9pt;
         table-layout: fixed;
         vertical-align: middle;
         }
         td p {
               align: left
         }
         table.hdr-desc {
         margin-top:10px;
         }
         table.hdr-desc tr td {
         padding: 5px;
         font-weight:bold;
         font-size:10pt;
         }
         table.hdr-desc tr td:first-child {
         color: #808080;
         }
         table.billto-shipto {
         width:85%;
         margin-top:10px;
         font-weight:bold;   
         }
         table.billto-shipto tr td {
         font-weight: bold;
         padding:6px;
         border: 1px solid black;
         border-bottom:none;
         }
         table.billto-shipto tr:last-child td {
         border-bottom: 1px solid black;
         }
         table.section-2 tr td{
         padding:6px;
         border: 1px solid black;
         border-bottom:none;
         border-right:none;
         }
         table.section-2 tr:nth-child(odd) td{
         color: #808080;
         }
         table.section-2 tr:nth-child(even) td{
         color: black;
         }
         table.section-2 tr td:last-child{
         border-right: 1px solid black;
         }
         table.section-2 tr:last-child td{
         border-bottom: 1px solid black;
         }
         table.item-table th {
         font-weight: bold;
         padding: 8px;
         border:1px solid black;
         border-right:none;
         }
         table.item-table th:last-child {
         border-right:1px solid black;
         }
         table.item-table td {
         font-size:8pt;
         padding: 6px;
         border:1px solid black;
         <!--  border-top:none; -->
         border-right:none;
         }
         table.item-table td:last-child {
         border-right:1px solid black;
         }
         table.installment-table {
         padding:10px;
         border: 1px solid black;
         <!--  border-top:none; -->
         }
         table.installment-table td {
         font-size:8pt;
         padding: 6px;
         }
         table.remittance-information-table td {
         border: 1px solid black;
         border-collapse: collapse;
         font-size: 8pt;
         }
      </style>
   </head>
   <body header="nlheader" header-height="18%" footer="nlfooter" footer-height="8%" padding="0.5in 0.5in 0.5in 0.5in" size="Letter">
      
      <table style="font-weight:bold; padding-bottom:10px; padding-top:10px; margin-bottom:5px;" width="100%">
         <tr>
            <td style="font-weight:bold; font-size: 14pt;"> LieferSchein ${record.tranid} </td>
         </tr>
        <tr>
          <td align = "right"> Datum: ${record.trandate} </td>
        </tr>
         <tr>
            <td>Auftrags- Nr: ${salesorder.tranid} <br /> Kunden Nr: ${record.entity?keep_before(" ")} </td>
         </tr>
      </table>

      <table style="margin-top:15px; padding-bottom:10px;" width="100%">
        <tr>
		   <td align = "left" style="font-weight:bold;">
		    Lieferadresse </td>
          </tr>
        <tr>
           <td align = "left" style="font-weight:bold;">

              ${record.shipaddress}
           </td>
          </tr>
        
     </table>

      <table class="item-table" style="margin-top:10px;" width="100%">
         <tr>
            <th align="center" colspan="2" width="10%">Pos</th>
            <th align="center" colspan="2" width="20%">Art- Nr.</th>
            <th align="center" colspan="4" width="30%">Bezeichnung</th>
            <th align="center" colspan="1" width="10%">Menge</th>
            <th align="center" colspan="1" width="10%">Elnhiet</th>
         </tr>
         <#if record.item?has_content><!-- start items --><#list record.item as item>
         <tr>
           <td align="center" colspan="2" width="20%">${item_index + 1}</td>
            <td align="center" colspan="2" width="20%">${item.item}</td>
            <td align="center" colspan="4" width="30%">${item.description}</td>
            <td align="center" colspan="1" width="10%">${item.quantity}</td>
            <td align="center" colspan="1" width="10%">${item.uom}</td>
         </tr>
         </#list></#if>
      </table>
   </body>
</pdf>	