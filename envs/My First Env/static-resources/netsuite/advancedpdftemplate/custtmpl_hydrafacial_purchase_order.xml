<?xml version="1.0"?><!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
<head>
	<link name="NotoSans" type="font" subtype="truetype" src="${nsfont.NotoSans_Regular}" src-bold="${nsfont.NotoSans_Bold}" src-italic="${nsfont.NotoSans_Italic}" src-bolditalic="${nsfont.NotoSans_BoldItalic}" bytes="2" />
	<#if .locale == "zh_CN">
		<link name="NotoSansCJKsc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKsc_Regular}" src-bold="${nsfont.NotoSansCJKsc_Bold}" bytes="2" />
	<#elseif .locale == "zh_TW">
		<link name="NotoSansCJKtc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKtc_Regular}" src-bold="${nsfont.NotoSansCJKtc_Bold}" bytes="2" />
	<#elseif .locale == "ja_JP">
		<link name="NotoSansCJKjp" type="font" subtype="opentype" src="${nsfont.NotoSansCJKjp_Regular}" src-bold="${nsfont.NotoSansCJKjp_Bold}" bytes="2" />
	<#elseif .locale == "ko_KR">
		<link name="NotoSansCJKkr" type="font" subtype="opentype" src="${nsfont.NotoSansCJKkr_Regular}" src-bold="${nsfont.NotoSansCJKkr_Bold}" bytes="2" />
	<#elseif .locale == "th_TH">
		<link name="NotoSansThai" type="font" subtype="opentype" src="${nsfont.NotoSansThai_Regular}" src-bold="${nsfont.NotoSansThai_Bold}" bytes="2" />
	</#if>
    <macrolist>
        <macro id="nlheader">
            <table class="header" style="width: 100%;"><tr>
	<td rowspan="3"><#if companyInformation.logoUrl?length != 0><img src="${companyInformation.logoUrl}" style="float: left; margin: 3px; width:70%;height:70%;" /> <span style="font-size:9px;"></span> </#if> </td>
	<td align="right"><span class="title">${record@title}</span></td>
	</tr>
	<tr>
	<td align="right"><span class="number">#${record.tranid}</span></td> 
	</tr>
	<tr>
	<td align="right">${record.trandate}</td>
	</tr>
        </table>
        </macro>
        <macro id="nlfooter">
            <table class="footer" style="width: 100%;"><tr>
	<td><barcode codetype="code128" showtext="true" value="${record.tranid}"/></td>
	<td align="right"><pagenumber/> of <totalpages/></td>
	</tr></table>
        </macro>
    </macrolist>
    <style type="text/css">* {
		<#if .locale == "zh_CN">
			font-family: NotoSans, NotoSansCJKsc, sans-serif;
		<#elseif .locale == "zh_TW">
			font-family: NotoSans, NotoSansCJKtc, sans-serif;
		<#elseif .locale == "ja_JP">
			font-family: NotoSans, NotoSansCJKjp, sans-serif;
		<#elseif .locale == "ko_KR">
			font-family: NotoSans, NotoSansCJKkr, sans-serif;
		<#elseif .locale == "th_TH">
			font-family: NotoSans, NotoSansThai, sans-serif;
		<#else>
			font-family: NotoSans, sans-serif;
		</#if>
		}
		table {
			font-size: 9pt;
			table-layout: fixed;
		}
        th {
            font-weight: bold;
            font-size: 8pt;
            vertical-align: middle;
            padding: 5px 6px 3px;
            background-color: #e3e3e3;
            color: #333333;
        }
        td {
            padding: 4px 6px;
        }
		td p { align:left }
        b {
            font-weight: bold;
            color: #333333;
        }
        table.header td {
            padding: 0;
            font-size: 10pt;
        }
        table.footer td {
            padding: 0;
            font-size: 8pt;
        }
        table.itemtable th {
            padding-bottom: 10px;
            padding-top: 10px;
        }
        table.body td {
            padding-top: 2px;
        }
        table.total {
            page-break-inside: avoid;
        }
        tr.totalrow {
            background-color: #e3e3e3;
            line-height: 200%;
        }
        td.totalboxtop {
            font-size: 12pt;
            background-color: #e3e3e3;
        }
        td.addressheader {
            font-size: 8pt;
            padding-top: 6px;
            padding-bottom: 2px;
        }
        td.address {
            padding-top: 0;
        }
        td.totalboxmid {
            font-size: 28pt;
            padding-top: 20px;
            background-color: #e3e3e3;
        }
        td.totalboxbot {
            background-color: #e3e3e3;
            font-weight: bold;
        }
        span.title {
            font-size: 26pt;
        }
        span.number {
            font-size: 16pt;
        }
        span.itemname {
            font-weight: bold;
            line-height: 150%;
        }
        hr {
            width: 100%;
            color: #d3d3d3;
            background-color: #d3d3d3;
            height: 1px;
        }
</style>
</head>
<body header="nlheader" header-height="10%" footer="nlfooter" footer-height="20pt" padding="0.5in 0.5in 0.5in 0.5in" size="Letter">
    <table style="width: 100%;"><tr>
	<td class="addressheader" colspan="6"><b>${record.billaddress@label}</b></td>
	<td class="addressheader" colspan="6"><b>Ship to</b></td>
	<td class="totalboxtop" colspan="5"><b>${record.total@label?upper_case}</b></td>
	</tr>
	<tr>
	<td class="address" colspan="6" rowspan="2">${record.billaddress}</td>
	<td class="address" colspan="6" rowspan="2">${record.shipaddress}</td>
	<td align="right" class="totalboxmid" colspan="5">${record.total}</td>
	</tr>
	<tr></tr></table>

<table class="body" style="width: 100%;"><tr>
	<th>Buyer</th>
	
	<th>${record.entity@label}</th>
	<th>${record.memo@label}</th>
	<th>${record.terms@label}</th>
  	<th>${record.entity.incoterm@label}</th>
	<th>${record.shipmethod@label}</th>
	</tr>
	<tr>
	<td>${record.custbody_hf_buyer}</td>

	<td>${record.entity.id}</td>
	<td>${record.memo}</td>
	<td>${record.terms}</td>
    <td>${record.incoterm}</td>
	<td>${record.shipmethod}</td>
	</tr></table>
<#if record.item?has_content>

<table class="itemtable" style="width: 100%;"><!-- start items --><#list record.item as item><#if item_index==0>
<thead>
	<tr>
	<th align="center" colspan="3">Line #</th>
	<th colspan="9">${item.item@label}</th>
    <th align="center" colspan="3">Mfg Part Number</th>
	<th align="center" colspan="3">Delivery Date</th>
	<th align="center" colspan="4">${item.custcol_hf_revision@label}</th>
	<th align="center" colspan="4">${item.quantity@label}</th>
	<th align="center" colspan="4">${item.rate@label}</th>
	<th align="center" colspan="4">${item.amount@label}</th>
	</tr>
</thead>
</#if><tr>
	<td align="center" colspan="3">${item.line}</td>
	<td colspan="9"><span class="itemname">${item.item}</span><br />${item.description}</td>
    <td align="center" colspan="3">${item.vendorname}</td>
    <td align="center" colspan="4">${item.custcol_hf_original_promise_date}</td>
	<td align="center" colspan="3">${item.custcol_hf_revision}</td>
	<td align="center" colspan="4" line-height="150%">${item.quantity}</td>
	<td align="center" colspan="4">${item.rate}</td>
	<td align="center" colspan="5">${item.amount}</td>
	</tr>
	</#list><!-- end items --></table>
</#if><#if record.expense?has_content>

<table class="itemtable" style="width: 100%;"><!-- start expenses --><#list record.expense as expense ><#if expense_index==0>
<thead>
	<tr>
	<th colspan="4">${expense.category@label}</th>
	<th colspan="10">${expense.account@label}</th>
	<th colspan="10">${expense.memo@label}</th>
	<th align="right" colspan="4">${expense.amount@label}</th>
	</tr>
</thead>
</#if><tr>
	<td colspan="4">${expense.category}</td>
	<td colspan="10"><span class="itemname">${expense.account}</span></td>
	<td colspan="10">${expense.memo}</td>
	<td align="right" colspan="4" style="white-space: nowrap;">${expense.amount}</td>
	</tr>
	</#list><!-- end expenses --></table>
</#if>

<hr />
<table class="total" style="width: 100%;"><tr class="totalrow">
	<td background-color="#ffffff" colspan="4">&nbsp;</td>
	<td align="right"><b>${record.total@label}</b></td>
	<td align="right">${record.total}</td>
	</tr></table>
<br /><br /><br /><span style="font-size: 9pt;"><b>Terms and Conditions</b> </span>

<p style="font-size: 9pt;"><span style="font-size: 9pt;">This Order includes all written specifications and drawings and all other requirements and descriptions regarding the ordered items that are attached to this Order or that have been separately delivered by The HydraFacial Company (&ldquo;HydraFacial&rdquo;) and expressly identified by it as being a part of this Order. All such specifications, drawings, and other item requirements and descriptions are hereby incorporated into this Order by this reference. In addition, the PURCHASE ORDER STANDARD TERMS AND CONDITIONS of EDGE SYSTEMS LLC dba THE HYDRAFACIAL COMPANY (revised November 2020), are incorporated herein by this reference and apply to this Order. A copy of those STANDARD TERMS AND CONDITIONS, if not attached hereto, will be provided to Supplier upon request. </span></p>
<span style="font-size: 9pt;"> </span>

  <#if record.subsidiary.id = '11'>
    <p style="font-size: 9pt;"><span style="font-size: 9pt;">More information is available at: https://hydrafacial.co.uk/terms-and-conditions/ </span></p>
                        <#else>
<p style="font-size: 9pt;"><span style="font-size: 9pt;">More information is available at: https://hydrafacial.com/supplier/ </span></p>
                  </#if>
    
<span style="font-size: 9pt;"> </span>
</body>
</pdf>