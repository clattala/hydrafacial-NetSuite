<?xml version="1.0"?><!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
<head>
	<link name="NotoSans" type="font" subtype="truetype" src="${nsfont.NotoSans_Regular}" src-bold="${nsfont.NotoSans_Bold}" src-italic="${nsfont.NotoSans_Italic}" src-bolditalic="${nsfont.NotoSans_BoldItalic}" bytes="2" />
    <macrolist>
        <macro id="nlheader">
            <table width="100%"><tr>
	<td width="60%">
	<table style="width: 100%; margin-bottom:0px; padding-bottom:0px;">
								<tr>
									<td width="100%">
										<#if companyInformation.logoUrl?length !=0><img src="https://6248126.app.netsuite.com/core/media/media.nl?id=2969912&amp;c=6248126&amp;h=QtwRNFc7ROYorrc4HNq0Lfg_ytkDPIfDktuxrgFkaL8Ig_Ww"
												style="position: relative; top: -18px; left: -22px; margin: 10px; width:20%;height:20%" /> </#if>
									</td>
								</tr>
							</table>
							<table style="width: 100%; position: relative; top: -10px;">
								<tr>
									<td width="50%">
										<table style="font-weight:bold; padding-bottom:10px; margin-bottom:5px;"
											width="100%">
											<tr>
												<td>${subsidiary.shippingaddress_text}</td>
											</tr>
                                          <tr>                                              <td>Phone: 800-603-4996</td>
</tr>
											
										</table>
									</td>
									<td width="50%">
										<table style="font-weight:bold; padding-bottom:10px;" width="100%">
											<tr>
												
											</tr>
											
											<tr>
												<td>&nbsp;</td>
											</tr>
										</table>
									</td>
								</tr>
							</table>
	</td>
	<td align="right" width="40%">
	<table width="50%"><tr>
		<td style="width:95%; font-size:16px; font-weight:bold;">Credit Memo</td>
		</tr></table>

	<table class="hdr-desc" width="100%"><tr>
		<td><b>Customer</b></td>
		<td>${record.entity?keep_before(" ")}</td>
		</tr>
		<tr>
          <td><b>CreditMemo No:</b></td>
		<td>${record.tranid}</td>
		</tr>
		<tr>
		<td><b>Credit Memo Date:</b></td>
		<td>${record.trandate}</td>
		</tr>
		
		<tr>
		<td><b>Page:</b></td>
		<td><pagenumber/> of <totalpages/></td>
		</tr></table>
	</td>
	</tr></table>
        </macro>
        <macro id="nlfooter">
            <table style="margin-top:10px;" width="100%">
              <tr>
                <td align="center">For all customer needs: 800-603-4996 Customer Care Option 2 Technical Support Option 3 Credit and Collections</td>
              </tr>
              
          </table>
        </macro>
    </macrolist>
    <style type="text/css">* {
        font-family: NotoSans, sans-serif;
		}
		
      table.hdr-desc {
            margin-top:10px;
        }

        table.hdr-desc tr td {
            padding: 5px;
            font-size:10pt;
        }

        
        table {
            width:100%;
			font-size: 9pt;
			table-layout: fixed;
            vertical-align: middle;
            align: center;
		}

        table.item-table th {
            font-weight: bold;
            padding: 8px;
            border:1px black;
            border-right:none;
        }

        table.item-table th:last-child {
            border-right:1px black;
        }

        table.item-table td {
            font-size:8pt;
            padding: 6px;
            border:1px black;
            <!--  border-top:none; -->
            border-right:none;
        }

        table.item-table td:last-child {
            border-right:1px black;
        }

        .light {
            color: darkgrey;
            font-size: 8px;
        }

        table.item-total tr {
            border-bottom:1px dotted #0D47A1;
        }

        table.item-total tr:last-child {
            border-bottom:2px #0D47A1;
        }

        table.tbl-top-right tr {
            color:darkgrey;
            padding: 3px 0;  
        }
 table.billto-shipto {
            width:95%;
            margin-top:10px;
        }

        table.billto-shipto tr td {
            
            padding:6px;
            border: 1px  black;
            border-bottom:none;
        }

        table.billto-shipto tr:last-child td {
            border-bottom: 1px  black;
        }
        table.tbl-top-right tr.data-layer {
            color:black;
           
            border-bottom:1px dotted #0D47A1; 
        }
       table.total {
            page-break-inside: avoid;
        }
        tr.totalrow {
            background-color: #e3e3e3;
            line-height: 200%;
        }
        td.totalboxtop {
            font-size: 12pt;
            background-color: #e3e3e3;
        }
       table.section-2 tr td{
            padding:6px;
            border: 1px black;
            border-bottom:none;
            border-right:none;
        }

        

        table.section-2 tr:nth-child(even) td{
            color: black;
        }

        table.section-2 tr td:last-child{
            border-right: 1px black;
        }

        table.section-2 tr:last-child td{
            border-bottom: 1px black;
        }


</style>
</head>
<body header="nlheader" header-height="25%" footer="nlfooter" footer-height="8%" padding="0.5in 0.5in 0.5in 0.5in" size="Letter">
<table style="margin-top:5px;" width="100%"><tr>
	<td align="left">
	<table class="billto-shipto"><tr>
		<td><b>Bill To </b></td>
		</tr>
		<tr>
		<td height="100px">${record.billaddress}</td>
		</tr></table>
	</td>
	<td align="right">
	<table class="billto-shipto"><tr>
		<td><b>Ship To </b></td>
		</tr>
		<tr>
		<td height="100px">${record.shipaddress}</td>
		</tr></table>
	</td>
	</tr></table>
  <br></br>

  <table class="section-2" style="margin-top:15px;" width="100%"><tr>
	<td align="center"><b>Customer ID</b></td>
	<td align="center"><b>Customer Name</b></td>
	<td align="center"><b>PO Number</b></td>
	</tr>
	<tr>
	<td align="center">${record.entity?keep_before(" ")}</td>
	<td align="center">${record.entity?keep_after(" ")}</td>
	<td align="center">${record.otherrefnum}</td>
	</tr>
    
  </table>
  <br></br>

   <table class="item-table" style="margin-top:10px;" width="100%"><tr>
	<th align="center" colspan="1" width="10%">Quantity</th>
	<#if record.shipcountry = "CA">
		<th align="center" colspan="2" width="15%">Item No.</th>
		<th align="center" colspan="4" width="20%">Description</th>
		<th  align="center" colspan="1" width="7%">
			Tax<br />Rate
		</th>
		<th  align="center" colspan="1" width="10%">
			Tax<br />Amount
		</th>
		<th align="center" colspan="1" width="14%">Unit<br />Price</th>
		<th align="center" colspan="1" width="14%">Amount</th>
	<#else>
		<th align="center" colspan="2" width="20%">Item No.</th>
		<th align="center" colspan="3" width="30%">Description</th>
		<th align="center" colspan="1" width="15%">Unit<br />Price</th>
		<th align="center" colspan="1" width="15%">Amount</th>
	</#if>
	</tr>
	<#if record.item?has_content><#list record.item as item>
	<tr>
	<td align="center" colspan="1" width="10%">${item.quantity}</td>
	<#if record.shipcountry = "CA">
		<td align="center" colspan="2" width="15%">${item.item}</td>
		<td align="left" colspan="4" width="20%">${item.description}</td>
		<td  align="center" colspan="1" width="7%">
			${item.taxrate1}
		</td>
		<td  align="center" colspan="1" width="10%">
          	<#if item.taxrate1?has_content>
              <#assign percentage = item.taxrate1?replace('%', '')?number / 100>
              <#assign number = item.amount>
              <#assign result = number * percentage>
              ${item.amount?keep_before(" ")} ${result?string.currency}
            <#else>
				&nbsp;
			</#if>
		</td>
		<td align="center" colspan="1" width="14%">${item.rate}</td>
		<td align="center" colspan="1" width="14%">${item.amount}</td>
	<#else>
		<td align="center" colspan="2" width="20%">${item.item}</td>
		<td align="left" colspan="3" width="30%">${item.description}</td>
		<td align="center" colspan="1" width="15%">${item.rate}</td>
		<td align="center" colspan="1" width="15%">${item.amount}</td>
	</#if>
	</tr>
	</#list></#if>
</table>
  
<table class="total" style="width: 100%; margin-top: 10px;"><tr>
	<td colspan="4">&nbsp;</td>
	<td align="right"><b>${record.subtotal@label}</b></td>
	<td align="right">${record.subtotal}</td>
	</tr>
	<tr>
	<td colspan="4">&nbsp;</td>
	<td align="right"><b>VAT Total</b></td>
	<td align="right">${record.taxtotal}</td>
	</tr>
	<tr class="totalrow">
	<td background-color="#ffffff" colspan="4">&nbsp;</td>
	<td align="right"><b>${record.total@label}</b></td>
	<td align="right">${record.total}</td>
	</tr></table>
      <table class="total" style="width: 100%; margin-top: 10px;">
                <#if record.apply?has_content><#list record.apply as apply>
                  <#if apply_index==0>
<tr>
  
	<td colspan="4">&nbsp;</td>
	<td align="right"><b>Created From</b></td>
	<td align="right">${apply.type}</td>
	</tr>
                    </#if>
	<tr>
	<td colspan="4">&nbsp;</td>
                        <#if apply_index==0>

	<td align="right"><b>Ref Num</b></td>
                          <#else>
                            <td align="right"> <b>       </b></td>
                          </#if>
	<td align="right">${apply.refnum}</td>
	</tr>
                  </#list></#if>
	
	</table>
</body>
</pdf>