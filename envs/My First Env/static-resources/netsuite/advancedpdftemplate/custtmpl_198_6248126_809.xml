<?xml version="1.0"?><!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd"><pdfset><#if statements?has_content><#list statements as statement>
<pdf>
<head>
	<link name="NotoSans" type="font" subtype="truetype" src="${nsfont.NotoSans_Regular}" src-bold="${nsfont.NotoSans_Bold}" src-italic="${nsfont.NotoSans_Italic}" src-bolditalic="${nsfont.NotoSans_BoldItalic}" bytes="2" />
	<#if .locale == "zh_CN">
		<link name="NotoSansCJKsc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKsc_Regular}" src-bold="${nsfont.NotoSansCJKsc_Bold}" bytes="2" />
	<#elseif .locale == "zh_TW">
		<link name="NotoSansCJKtc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKtc_Regular}" src-bold="${nsfont.NotoSansCJKtc_Bold}" bytes="2" />
	<#elseif .locale == "ja_JP">
		<link name="NotoSansCJKjp" type="font" subtype="opentype" src="${nsfont.NotoSansCJKjp_Regular}" src-bold="${nsfont.NotoSansCJKjp_Bold}" bytes="2" />
	<#elseif .locale == "ko_KR">
		<link name="NotoSansCJKkr" type="font" subtype="opentype" src="${nsfont.NotoSansCJKkr_Regular}" src-bold="${nsfont.NotoSansCJKkr_Bold}" bytes="2" />
	<#elseif .locale == "th_TH">
		<link name="NotoSansThai" type="font" subtype="opentype" src="${nsfont.NotoSansThai_Regular}" src-bold="${nsfont.NotoSansThai_Bold}" bytes="2" />
	</#if>
    <macrolist>
        <macro id="nlheader">
            <table class="header" style="width: 100%;"><tr>
	<td rowspan="3"><#if companyInformation.logoUrl?length != 0><img src="https://6248126-sb3.secure.netsuite.com/core/media/media.nl?id=9181187&amp;c=6248126_SB3&amp;h=g--8JvvpUgSpcZexY1ZkvJARIxwRkq3uGui_lVc0jTMxpjmJ" style="float: left; margin-left: -5px; height: 45px; width: 150px;" /> </#if><br /><br /><br /><br /><br /><br />Hydrafacial Germany GmbH<br />Stichlingstra&szlig;e 1<br />60327 Frankfurt am Main<br />Germany<br /><a data-auth="NotApplicable" data-linkindex="0" data-ogsc="" data-safelink="true" href="mailto:sm-emea-de-customerservice@hydrafacial.com" rel="noopener noreferrer" target="_blank">sm-emea-de-customerservice@hydrafacial.com</a><br />&nbsp;</td>
	<td align="right"><span style="font-size:36px;">Kontoauszug</span></td>
	</tr>
	<tr>
	<td align="right">${statement.trandate?string("dd/MM/yyyy")}</td>
	</tr>
	<tr>
	<td align="right">Kunden Nr.&nbsp; ${customer.entityid?substring(0,7)}</td>
	</tr></table>
        </macro>
        <macro id="nlfooter">
            <table class="footer" style="width: 100%;"><tr>
	<td align="right"><pagenumber/> of <totalpages/></td>
	</tr></table>
        </macro>
    </macrolist>
    <style type="text/css">* {
			<#if .locale == "zh_CN">
				font-family: NotoSans, NotoSansCJKsc, sans-serif;
			<#elseif .locale == "zh_TW">
				font-family: NotoSans, NotoSansCJKtc, sans-serif;
			<#elseif .locale == "ja_JP">
				font-family: NotoSans, NotoSansCJKjp, sans-serif;
			<#elseif .locale == "ko_KR">
				font-family: NotoSans, NotoSansCJKkr, sans-serif;
			<#elseif .locale == "th_TH">
				font-family: NotoSans, NotoSansThai, sans-serif;
			<#else>
				font-family: NotoSans, sans-serif;
			</#if>
		}
		table {
			font-size: 9pt;
			table-layout: fixed;
		}
         th {
             font-weight: bold;
             font-size: 8pt;
             vertical-align: middle;
             padding: 5px 6px 3px;
             background-color: #e3e3e3;
             color: #333333;
         }
         td {
             padding: 4px 6px;
         }
		 td p { align:left }
         b {
             font-weight: bold;
             color: #333333;
         }
         table.header td {
             padding: 0;
             font-size: 10pt;
         }
         table.footer td {
             padding: 0;
             font-size: 8pt;
         }
         table.itemtable th {
             padding-bottom: 10px;
             padding-top: 10px;
         }
         table.body td {
             padding-top: 2px;
         }
         td.addressheader {
             font-weight: bold;
             font-size: 8pt;
             padding-top: 6px;
             padding-bottom: 2px;
         }
         td.address {
             padding-top: 0;
         }
         span.title {
             font-size: 28pt;
         }
         span.number {
             font-size: 16pt;
         }
         div.remittanceSlip {
             width: 100%;
             /* To ensure minimal height of remittance slip */
             height: 200pt;
             page-break-inside: avoid;
             page-break-after: avoid;
         }
         hr {
             border-top: 1px dashed #d3d3d3;
             width: 100%;
             color: #ffffff;
             background-color: #ffffff;
             height: 1px;
         }
</style>
</head>
<body header="nlheader" header-height="20%" footer="nlfooter" footer-height="20pt" padding="0.5in 0.5in 0.5in 0.5in" size="Letter">
    <#if customer.creditholdoverride == "On">
<table border="1" style="width: 100%; margin-top: 10px;"><tr>
	<td style="text-align: center; font-size:20px; color:red;">CUSTOMER IS CURRENTLY ON HOLD !</td>
	</tr></table>
</#if>

<table style="width: 100%; margin-top: 10px;"><tr>
	<td class="addressheader" colspan="3">Rechnungsadresse</td>
	</tr>
	<tr>
	<td class="address" colspan="3">${statement.billaddress}</td>
	</tr></table>

<table border="0" cellpadding="0" cellspacing="0" style="height: 1px; width: 85%; margin-top: -12px;"><tr>
	<td>
	<table cellspacing="0" style="width: 97.2455%;"><tr>
		<td colspan="3" style="height: 1px; width: 92.5276%;"><br /><strong>&nbsp;</strong></td>
		</tr></table>
	</td>
	</tr></table>

<table style="width: 100%;"><tr>
	<td><#if customer.currency.id = '2'>${subsidiary.custrecord_hf_sub_bank_details}</#if> <#if customer.currency.id = '4'>${subsidiary.custrecord_hf_sub_sec_bank_details}</#if></td>
	</tr></table>

<table class="body" style="width: 100%;"><tr>
	<th align="right">F&auml;lliger Betrag</th>
	</tr>
	<tr>
	<td align="right">${statement.amountDue}</td>
	</tr></table>
<#if statement.lines?has_content>

<table class="itemtable" style="width: 100%; margin-top: 10px;"><!-- start items --><#list statement.lines as line><#if line_index==0><#assign nday =.now?long />
<thead>
	<tr>
	<th colspan="4">Datum</th>
	<th colspan="5">Beschreibung</th>
	<th colspan="3">Tage<br />&uuml;berf&auml;llig</th>
	<th colspan="3">Zalhbar bis</th>
	<th colspan="3">Betrag</th>
	<th colspan="3">Zahlung</th>
	<th colspan="3">Saldo</th>
	</tr>
</thead>
</#if><#if !line.description?contains("Journal")><tr>
	<td colspan="4">${line.datecol?string("dd/MM/yyyy")}</td>
	<td colspan="5">${line.description}</td>
	<#setting date_format="yyyy-MM-dd"><#assign aday = line.duedate /><#if line.description?contains("Invoice")><#if line.duedate?trim != ''><#assign bday = line.duedate + 'T00:00:00.000Z' /><#assign cday = bday?datetime.iso?string("yyyy-MM-dd HH:mm:ss zzz")/><#assign dday = bday?datetime.iso?long/><#assign xday = statement.trandate + 'T00:00:00.000Z' /><#assign yday = xday?datetime.iso?string("yyyy-MM-dd HH:mm:ss zzz")/><#assign zday = xday?datetime.iso?long/><#assign ddiff = (zday-dday)/(1000 * 60 * 60 *  24) /><#assign ddiffr = ddiff?round />
	<td colspan="3">${ddiffr}</td>
	<#else></#if><#else></#if>
	<td colspan="3"><#if line.description?contains("Invoice")><#if line.duedate?trim != ''>${line.duedate?string("dd/MM/yyyy")}<#else>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</#if></#if></td>
	<td colspan="3">${line.charge}</td>
	<td colspan="3">${line.payment}</td>
	<td colspan="3">${line.balance}</td>
	</tr>
	</#if></#list><!-- end items --></table>

<table class="aging" style="width: 100%; margin-top: 10px;"><tr>
	<th>Nicht f&auml;llig</th>
	<th>1-30 Tage</th>
	<th>31-60 Tage</th>
	<th>61-90 Tage</th>
	<th>&Uuml;ber 90 Tage</th>
	<th>F&auml;lliger Betrag</th>
	</tr>
	<tr>
	<td>${statement.aging1}</td>
	<td>${statement.aging2}</td>
	<td>${statement.aging3}</td>
	<td>${statement.aging4}</td>
	<td>${statement.aging5}</td>
	<td>${statement.agingbal}</td>
	</tr></table>
&nbsp;

<table class="total" style="width: 100%; margin-top: 10px;"><tr>
	<td align="left" style="width: 150px;"><b>Kredit Pr&uuml;fer:</b></td>
	<td align="left" style="width: 180px;">${customer.custentity_3rp_cre_con}</td>
	<td colspan="3" style="width: 448px;">&nbsp;</td>
	</tr>
	<tr>
<!--	<td align="left" style="width: 150px;"><b>Telefonnummer:</b></td>
	<td align="left" style="width: 180px;">${customer.custentity_hf_3rp_cre_con_num}</td>
	<td colspan="3" style="width: 448px;">&nbsp;</td>
	</tr>
	<tr>-->
    <td align="left" style="width: 150px;"><b>Email Kontakt: </b></td>
	<td align="left" style="width: 180px;"><a data-auth="NotApplicable" data-linkindex="0" data-ogsc="" data-safelink="true" href="mailto:AR-DE@hydrafacial.com" id="LPlnk398291" rel="noopener noreferrer" target="_blank">AR-DE@hydrafacial.com</a></td>
	<td colspan="3" style="width: 448px;">&nbsp;</td>
	</tr></table>
</#if></body></pdf></#list><#else><pdf><head></head><body>

<p>Please use Single Currency Advanced Printing Template</p>
</body>
</pdf></#if></pdfset>