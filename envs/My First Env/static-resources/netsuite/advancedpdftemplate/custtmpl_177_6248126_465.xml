<?xml version="1.0"?><!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
<head>
	<link name="NotoSans" type="font" subtype="truetype" src="${nsfont.NotoSans_Regular}" src-bold="${nsfont.NotoSans_Bold}" src-italic="${nsfont.NotoSans_Italic}" src-bolditalic="${nsfont.NotoSans_BoldItalic}" bytes="2" />
    <macrolist>
        <macro id="nlheader">
            <table width="100%" style="margin-bottom:20px; border-bottom:1px dotted #0D47A1;">
                <tr>

                    <td align="left">
                        <span style="font-size:14px; font-weight:bold;"> THE HYDRAFACIAL COMPANY </span>
                        <table width="100%" style="padding-bottom:10px; margin-bottom:5px;">
                            <tr><td> 3600 E.Burnett St. </td></tr>
                            <tr><td> Long Beach CA 90815 </td></tr>
                            <tr><td> USA </td></tr>
                        </table>

                        <table width="100%" style="padding-bottom:10px; margin-bottom:10px;">
                            <tr> <td> <span style="font-weight:bold;"> NVP </span> </td> </tr>
                            <tr>
                                <td> 
                                    ${record.billaddress}
                                </td>
                            </tr>

                        </table>

                    </td>

                    <td align="right">
                        <table width="100%" style="border-bottom:1px dotted #0D47A1;">
                            <tr>
                                <td style="width:30%; font-size:18px; font-weight:bold;">
                                    Original
                                </td>
                                <td style="background-color:#E1AD01; width:2%;">
                                    &nbsp;
                                </td>
                                <td align="right" style="width:68%; font-size:20px; font-weight:bold;padding-bottom:2px;">
                                    OUTGOING PAYMENT
                                </td>
                            </tr>
                        </table>

                        <table class="tbl-top-right" width="100%">
                            <tr>
                                <td> Document Number </td>
                                <td align="right"> Document Date </td>
                                <td align="right"> Page </td>
                            </tr>
                            <tr class="data-layer">
                                <td> ${record.tranid} </td>
                                <td align="right"> ${record.trandate} </td>
                                <td align="right"> <pagenumber/>/<totalpages/> </td>
                            </tr>

                            <tr>
                                <td> Customer No. </td>
                                <td align="right" colspan="2"> Federal Tax ID - Business Partner </td>
                            </tr>
                            <tr class="data-layer">
                                <td> ${record.entity} </td>
                                <td colspan="2"> &nbsp; </td>
                            </tr>
                            <tr> <td colspan="3"> Your Reference </td> </tr>
                            <tr class="data-layer"> <td colspan="3"> &nbsp; </td> </tr>
                            <tr> <td colspan="3"> Your Contact </td> </tr>
                            <tr> <td colspan="3"> &nbsp; </td> </tr>   
                        </table>

                    </td>

                </tr>
            </table>


        </macro>
        <macro id="nlfooter">
            <table width="100%" style="font-weight:light;font-size:7px;margin-top:10px; border-top:1px dotted #0D47A1;">
                <tr>
                    <td style="margin:0;padding:2px">

                        <table width="100%" style="border:0;">
                            <tr> 
                                <td> CEO: </td>
                                <td>  </td> 
                            </tr>
                            <tr> 
                                <td> Shareholder: </td>
                                <td>  </td> 
                            </tr>
                            <tr> 
                                <td> Tax Official: </td>
                                <td>  </td> 
                            </tr>
                            <tr> 
                                <td> Headerquarters: </td>
                                <td>  </td> 
                            </tr>
                            <tr> 
                                <td> Website: </td>
                                <td>  </td> 
                            </tr>
                        </table>

                    </td>

                    <td>

                        <table width="100%" style="margin:0;padding:2px">
                            <tr> 
                                <td> Phone: </td>
                                <td>  </td> 
                            </tr>
                            <tr> 
                                <td> Fax: </td>
                                <td>  </td> 
                            </tr>
                            <tr> 
                                <td> Email-Id: </td>
                                <td>  </td> 
                            </tr>
                            <tr> 
                                <td> Tax No.: </td>
                                <td>  </td> 
                            </tr>
                            <tr> 
                                <td> Tax ID No.: </td>
                                <td>  </td> 
                            </tr>
                        </table>

                    </td>

                    <td>

                        <table width="100%" style="margin:0;padding:2px">
                            <tr> 
                                <td> Bank Name: </td>
                                <td>  </td> 
                            </tr>
                            <tr> 
                                <td> Bank Account: </td>
                                <td>  </td> 
                            </tr>
                            <tr> 
                                <td> Bank Code: </td>
                                <td>  </td> 
                            </tr>
                            <tr> 
                                <td> Swift/BIC Code: </td>
                                <td>  </td> 
                            </tr>
                            <tr> 
                                <td> IBAN: </td>
                                <td>  </td> 
                            </tr>
                        </table>        

                    </td>
                </tr>
            </table>
        </macro>
    </macrolist>
    <style type="text/css">* {
        font-family: NotoSans, sans-serif;
		}
		
        table {
            width:100%;
			font-size: 9pt;
			table-layout: fixed;
            vertical-align: middle;
            align: center;
		}

        table.item-table tr td {
            padding: 10px;
        }

        table.item-table tr.rowend {
            border-bottom:2px solid #0D47A1;
        }

        .light {
            color: darkgrey;
            font-size: 8px;
        }

        table.item-total tr {
            border-bottom:1px dotted #0D47A1;
        }

        table.item-total tr:last-child {
            border-bottom:2px solid #0D47A1;
        }

        table.tbl-top-right tr {
            color:darkgrey;
            padding: 3px 0;  
        }

        table.tbl-top-right tr.data-layer {
            color:black;
            font-weight: bold;
            border-bottom:1px dotted #0D47A1; 
        }
</style>
</head>
<body header="nlheader" header-height="35%" footer="nlfooter" footer-height="8%" padding="0.5in 0.5in 0.5in 0.5in" size="Letter">

    <table width="100%" style="margin-top:10px;">
        <tr>
            <td style="background-color:#E1AD01;width:1%">  &nbsp; </td>
                <td style="font-weight:bold;"> Payments </td>
            </tr>
    </table>

    <table class="itemtable" style="width: 100%; margin:20px 0;border-bottom:2px solid #0D47A1;">
    <!-- start apply sublist -->
    <#list record.apply as apply>
    <#if apply_index==0>
    <thead>
        <tr style="font-weight:bold;border-bottom:1px dotted #0D47A1;">
            <td> # </td>
            <td> Paid Doc. </td>
            <td> Doc. No. </td>
            <td> Date </td>
            <td> Installment ID </td>
            <td> Reference Number </td>
            <td align="right"> Amount </td>
        </tr>
    </thead>
    </#if>
        <tr>
            <td> ${apply_index} </td>
            <td> ${apply.type} </td>
            <td> &nbsp; </td>
            <td> ${apply.applydate} </td>
            <td> &nbsp; </td>
            <td> &nbsp; </td>
            <td align="right"> ${apply.amount} </td>
	    </tr>
	</#list>
    <!-- end apply -->
    </table>

    <table style="font-weight:bold;margin-top:10px; font-size:14px;">
        <tr>
            <td width="50%"> </td>
            <td width="50%">
                <table>
                    <tr style="border-bottom:1px dotted #0D47A1;">
                        <td>
                            Document Amount Due:
                        </td>
                        <td align="right" >

                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>

    <table align="right" style="font-weight:bold;margin-top:10px; font-size:14px;">
        <tr>
            <td width="50%"> </td>
            <td width="50%">
                <table>
                    <tr style="border-bottom:1px solid #0D47A1;">
                        <td>
                            <span style="background-color:#E1AD01;width:1%;">  &nbsp; </span>
                            &nbsp; Total Amount Due:
                        </td>
                        <td align="right" >

                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>

    <table width="100%" style="margin-top:10px;">
        <tr>
            <td style="background-color:#E1AD01;width:1%">  &nbsp; </td>
            <td style="font-weight:bold;"> Credit Card </td>
        </tr>
    </table>

    <table style="border-bottom:1px solid #0D47A1;margin-top:10px;">
        <tr style="font-weight:bold;border-bottom:1px dotted #0D47A1;">
            <td> Card Name </td>
            <td> ID Number </td>
            <td> No. of Payments </td>
            <td> First Payment </td>
            <td> Payment </td>
            <td> Amount </td>
        </tr>
        <tr style="border-bottom:1px dotted #0D47A1;">
            <td> ${record.paymentmethod}  </td>
            <td>  </td>
            <td>  </td>
            <td>  </td>
            <td>  </td>
            <td>  </td>
        </tr>
    </table>

    <table style="font-weight:bold;margin:10px 0; font-size:14px;">
        <tr>
            <td width="50%"> </td>
            <td width="50%">
                <table>
                    <tr style="border-bottom:1px dotted #0D47A1;">
                        <td>
                            Credit Total:
                        </td>
                        <td align="right" >

                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>

    <table align="right" style="font-weight:bold;margin:20px 0;border-bottom:2px solid #0D47A1;font-size:14px;">
        <tr>
            <td width="50%"> </td>
            <td width="50%">
                <table>
                    <tr>
                        <td>
                            <span style="background-color:#E1AD01;width:1%;">  &nbsp; </span>
                            &nbsp; Total Payment Amount:
                        </td>
                        <td align="right" >
                            ${record.total}
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>

    <table style="color:darkgrey; font-weight:light;margin-top:20px;">
        <tr>
            <td>
                Outgoing Payments - ${record.customer?split(" ")[0]}
            </td>
            <td>
                <!--notes-->
            </td>
        </tr>
    </table>

</body>
</pdf>