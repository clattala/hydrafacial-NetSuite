<?xml version="1.0"?><!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
<head>
	<link name="NotoSans" type="font" subtype="truetype" src="${nsfont.NotoSans_Regular}" src-bold="${nsfont.NotoSans_Bold}" src-italic="${nsfont.NotoSans_Italic}" src-bolditalic="${nsfont.NotoSans_BoldItalic}" bytes="2" />
	<#if .locale == "zh_CN">
		<link name="NotoSansCJKsc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKsc_Regular}" src-bold="${nsfont.NotoSansCJKsc_Bold}" bytes="2" />
	<#elseif .locale == "zh_TW">
		<link name="NotoSansCJKtc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKtc_Regular}" src-bold="${nsfont.NotoSansCJKtc_Bold}" bytes="2" />
	<#elseif .locale == "ja_JP">
		<link name="NotoSansCJKjp" type="font" subtype="opentype" src="${nsfont.NotoSansCJKjp_Regular}" src-bold="${nsfont.NotoSansCJKjp_Bold}" bytes="2" />
	<#elseif .locale == "ko_KR">
		<link name="NotoSansCJKkr" type="font" subtype="opentype" src="${nsfont.NotoSansCJKkr_Regular}" src-bold="${nsfont.NotoSansCJKkr_Bold}" bytes="2" />
	<#elseif .locale == "th_TH">
		<link name="NotoSansThai" type="font" subtype="opentype" src="${nsfont.NotoSansThai_Regular}" src-bold="${nsfont.NotoSansThai_Bold}" bytes="2" />
	</#if>
    <macrolist>
        <macro id="nlheader">
            <table class="header" style="width:700.991px;"><tr>
	<td rowspan="3">&nbsp;<img src="https://6248126.app.netsuite.com/core/media/media.nl?id=5806&amp;c=6248126&amp;h=zZbi81LFizZvpjNItu6k0hIv3lqVAC5HF9gIZsORVpJXyJOm" style="float: left; margin-top: 5px; margin-bottom: 5px; height: 40px; width: 130px;" /><br /><br /><br /><br /><br />${record.shipaddress}</td>
	<td align="right" style="width: 216px;">&nbsp;</td>
	</tr>
	<tr>
	<td align="right" style="width: 216px;">&nbsp;</td>
	<td align="right" style="width: 275px;"><br />&nbsp;</td>
	</tr>
	<tr>
	<td align="right" style="width: 216px;">&nbsp;</td>
	<td align="right" style="width: 216px;">${subsidiary.mainaddress_text}<br />OrdersUK@hydrafacial.com</td>
	</tr>
	<tr>
	<td>&nbsp;</td>
	<td align="right" style="width: 216px;">&nbsp;</td>
	</tr>
	<tr>
	<td>&nbsp;</td>
	<#assign customercountry1 = record.shippingaddress.country><#if customercountry1?contains("Ireland") >
	<td align="right" style="width: 216px;">&nbsp;</td>
	</#if><#if customercountry1?contains("Ireland") == false>
	<td align="right" style="width: 275px;">&nbsp;</td>
	</#if></tr></table>
<br /><br /><br /><br /><br />&nbsp;
        </macro>
        <macro id="nlfooter">
            <table class="footer" style="width:100%;"><tr>
	<td style="height: 101px; width: 156px;"><barcode codetype="code128" showtext="true" value="${record.tranid}"/></td>
	<td style="height: 101px; width: 125px;"><span style="font-size:8px;">${subsidiary.custrecord_hf_sub_bank_details} <br />HRB 127913</span></td>
	<td align="right" style="width: 90px; height: 101px;"><span style="font-size:8px;"><pagenumber/>&nbsp;/&nbsp;<totalpages/></span></td>
	</tr></table>
        </macro>
    </macrolist>
    <style type="text/css">* {
		<#if .locale == "zh_CN">
			font-family: NotoSans, NotoSansCJKsc, sans-serif;
		<#elseif .locale == "zh_TW">
			font-family: NotoSans, NotoSansCJKtc, sans-serif;
		<#elseif .locale == "ja_JP">
			font-family: NotoSans, NotoSansCJKjp, sans-serif;
		<#elseif .locale == "ko_KR">
			font-family: NotoSans, NotoSansCJKkr, sans-serif;
		<#elseif .locale == "th_TH">
			font-family: NotoSans, NotoSansThai, sans-serif;
		<#else>
			font-family: NotoSans, sans-serif;
		</#if>
		}
		table {
			font-size: 9pt;
			table-layout: fixed;
		}
        th {
            font-weight: bold;
            font-size: 8pt;
            vertical-align: middle;
            padding: 5px 6px 3px;
            background-color: #e3e3e3;
            color: #333333;
          
        }
  table th {
    text-align: justify;
}
  table th {
    align: left;
}
        td {
            padding: 4px 6px;
        }
		th p { align:left }
        td p {align:left}
        
        b {
            font-weight: bold;
            color: #333333;
        }
        table.header td {
            padding: 0px;
            font-size: 10pt;
        }
        table.footer td {
            padding: 0px;
            font-size: 8pt;
        }
        table.itemtable th {
            padding-bottom: 10px;
            padding-top: 10px;
        }
        table.body td {
            padding-top: 2px;
        }
        table.total {
            page-break-inside: avoid;
        }
        tr.totalrow {
            background-color: #e3e3e3;
            line-height: 200%;
        }
        td.totalboxtop {
            font-size: 12pt;
            background-color: #e3e3e3;
        }
        td.addressheader {
            font-size: 8pt;
            padding-top: 6px;
            padding-bottom: 2px;
        }
        td.address {
            padding-top: 0px;
        }
        td.totalboxmid {
            font-size: 28pt;
            padding-top: 20px;
            background-color: #e3e3e3;
        }
        td.totalboxbot {
            background-color: #e3e3e3;
            font-weight: bold;
        }
        span.title {
            font-size: 28pt;
        }
        span.number {
            font-size: 16pt;
        }
        span.itemname {
            font-weight: bold;
            line-height: 150%;
        }
        hr {
            width: 100%;
            color: #d3d3d3;
            background-color: #d3d3d3;
            height: 1px;
        }
</style>
</head>
<body header="nlheader" header-height="22%" footer="nlfooter" footer-height="10pt" padding="0.5in 0.5in 0.5in 0.5in" size="Letter">
    <table style="width: 100%; margin-top: 10px;"><tr>
	<td class="addressheader" colspan="3" style="height: 23px;"><span style="font-size:16px;"><strong>Return Authorization</strong>&nbsp;</span><strong><span class="number"><span style="font-size:18px;"><span style="font-family:Verdana,Geneva,sans-serif;"><span class="number"><span style="font-size:18px;"><span style="font-family:Verdana,Geneva,sans-serif;"><span class="number"><span style="font-size:18px;"><span style="font-family:Verdana,Geneva,sans-serif;"><span class="number"><span style="font-size:18px;"><span style="font-family:Verdana,Geneva,sans-serif;">${record.tranid}</span></span></span></span></span></span></span></span></span></span></span></span></strong><br />&nbsp;</td>
	<td class="addressheader" colspan="3" style="height: 23px;">&nbsp;</td>
	</tr>
	<tr>
	<td class="address" colspan="3" rowspan="2" style="height: 20px;"><strong>Order No:&nbsp;</strong>${record.createdfrom?keep_after("Sales Order #")}<br /><strong>Customer No:&nbsp;</strong>${record.custbody_ava_customerentityid}</td>
	<td class="address" colspan="3" rowspan="2" style="height: 20px;">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;<strong>Date&nbsp;</strong>${record.trandate}<br /><strong>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;${record.saleseffectivedate@label} </strong>:&nbsp;${record.saleseffectivedate}</td>
	</tr>
	<tr></tr></table>

<table border="0" cellpadding="1" cellspacing="1" style="width:100%;"><tr>
	<td style="width: 394px; height: 0px;"><strong>&nbsp;&nbsp;</strong></td>
	<td style="width: 392px;">&nbsp;</td>
	</tr>
	<tr>
	<td style="width: 394px;">&nbsp;</td>
	<td style="width: 392px;">&nbsp;</td>
	</tr></table>
<br /><#if record.item?has_content>
<table class="itemtable" style="margin-top:10px;width:100%;"><!-- start items --><#list record.item as item><#if item_index==0>
<thead>
	<tr>
	<th align="center" colspan="3" style="width: 20px;">#</th>
	<th colspan="12" style="width: 47px;">${item.item@label}</th>
	<th colspan="12" style="width: 40px;">${item.description@label}</th>
	<th colspan="12" style="width: 20px;">${item.custcol_hf_serialno@label}</th>
	<th colspan="3" style="width: 30px;">${item.quantity@label}</th>
	<th align="right" colspan="4" style="width: 30px;">${item.units@label}</th>
	<th align="right" colspan="3" style="width: 30px;">${item.taxrate1@label}</th>
	<th align="right" colspan="3" style="width: 30px;">${item.rate@label}</th>
	<th  align="right" colspan="4" style="width: 25px;"> ${item.tax1amt@label}</th>
	<th align="right" colspan="4" style="width: 35px;">${item.grossamt@label}&nbsp;</th>
	</tr>
</thead>
</#if><tr>
	<td align="center" colspan="3" line-height="150%" style="width: 20px;"><span style="font-size:9px;">${item_index + 1}</span></td>
	<td colspan="12" style="width: 60px;"><span style="font-size:10px;"><span class="itemname">&nbsp;</span></span><span style="font-size:9px;"><span class="itemname">${item.item}</span></span></td>
	<td colspan="12" style="width: 60px;"><span style="font-size:9px; white-space: nowrap">${item.description}</span></td>
	<td colspan="12" style="width: 40px;"><span style="font-size:9px; white-space: nowrap ">${item.custcol_hf_serialno}</span></td>
	<td colspan="3" style="width: 20px; text-align: left;"><span style="font-size:10px;">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;&nbsp;</span><span style="font-size:9px; white-space: nowrap ">${item.quantity}</span></td>
	<td align="right" colspan="4" style="width: 20px;"><span style="font-size:9px; white-space: nowrap ">${item.units}</span></td>
	<td align="right" colspan="3" style="width: 25px;"><span style="font-size:9px; white-space: nowrap">${item.taxrate1}</span></td>
	<td align="right" colspan="3" style="width: 80px;"><span style="font-size:9px; white-space: nowrap">${item.rate}</span></td>
	<td align="right" colspan="4" style="width: 45px;"><span style="font-size:9px; white-space: nowrap">${item.tax1amt}</span></td>
	<td align="right" colspan="4" style="width: 80px;"><span style="font-size:9px; white-space: nowrap;" >&nbsp;${item.grossamt}</span></td>
	</tr>
	</#list><!-- end items --></table>

<hr /></#if>
<table class="total" style="width: 100%; margin-top: 10px;"><tr>
	<td align="left">&nbsp;</td>
	<td align="left">&nbsp;</td>
	<td colspan="2">&nbsp;</td>
	<td align="right">&nbsp;</td>
	<td align="right">&nbsp;</td>
	</tr>
	<tr>
	<td align="left">&nbsp;</td>
	<td align="left">&nbsp;</td>
	<td colspan="2">&nbsp;</td>
	<td align="right"><b>VAT Total</b></td>
	<td align="right">${record.taxtotal}</td>
	</tr>
	<tr class="totalrow">
	<td background-color="#ffffff" colspan="4">&nbsp;</td>
	<td align="right"><b>Total</b></td>
	<td align="right"><strong>${record.total}</strong></td>
	</tr></table>
</body>
</pdf>