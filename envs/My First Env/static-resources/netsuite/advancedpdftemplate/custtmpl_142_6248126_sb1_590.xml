    <style type="text/css">* {
        font-family: NotoSans, sans-serif;
		}
		
        table {
			font-size: 9pt;
			table-layout: fixed;
            vertical-align: middle;
            align: center;
		}
		td p { align:left }
        table.hdr-desc {
            margin-top:10px;
        }

        table.hdr-desc tr td {
            padding: 5px;
            font-weight:bold;
            font-size:10pt;
        }

        table.hdr-desc tr td:first-child {
            color: #808080;
        }

        table.billto-shipto {
            width:85%;
            margin-top:10px;
            font-weight:bold;   
        }

        table.billto-shipto tr td {
            font-weight: bold;
            padding:6px;
            border: 1px solid black;
            border-bottom:none;
        }

        table.billto-shipto tr:last-child td {
            border-bottom: 1px solid black;
        }

        table.section-2 tr td{
            padding:6px;
            border: 1px solid black;
            border-bottom:none;
            border-right:none;
        }

        table.section-2 tr:nth-child(odd) td{
            color: #808080;
        }

        table.section-2 tr:nth-child(even) td{
            color: black;
        }

        table.section-2 tr td:last-child{
            border-right: 1px solid black;
        }

        table.section-2 tr:last-child td{
            border-bottom: 1px solid black;
        }

        table.item-table th {
            font-weight: bold;
            padding: 8px;
            border:1px solid black;
            border-right:none;
        }

        table.item-table th:last-child {
            border-right:1px solid black;
        }

        table.item-table td {
            font-size:8pt;
            padding: 6px;
            border:1px solid black;
            <!--  border-top:none; -->
            border-right:none;
        }

        table.item-table td:last-child {
            border-right:1px solid black;
        }

        table.installment-table {
            padding:10px;
            border: 1px solid black;
          <!--  border-top:none; -->
        }

        table.installment-table td {
            font-size:8pt;
            padding: 6px;
        }

        table.remittance-information-table td {
            border: 1px solid black;
            border-collapse: collapse;
          	font-size: 8pt;
